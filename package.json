{
  "name": "retrocord",
  "version": "1.1.5",
  "description": "The best in life",
  "main": "src/index.js",
  "bin": {
    "retrocord": "src/index.js"
  },
  "scripts": {
    "nexe": "node ./node_modules/nexe/bin/nexe"
  },
  "dependencies": {
    "ansi-256-colors": "^1.1.0",
    "discord.js": "github:hydrabolt/discord.js#master",
    "inquirer": "^2.0.0",
    "node-emoji": "^1.4.3",
    "ora": "^1.0.0",
    "username": "^2.2.2",
    "vorpal": "^1.11.4"
  },
  "optionalDependencies": {
    "erlpack": "github:hammerandchisel/erlpack"
  },
  "devDependencies": {
    "bufferutil": "^1.3.0",
    "nexe": "^1.1.2",
    "opusscript": "0.0.2",
    "utf-8-validate": "^2.0.0"
  },
  "author": "Gus Caplan <me@gus.host>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GusCaplan/retrocord.git"
  },
  "bugs": {
    "url": "https://github.com/GusCaplan/retrocord/issues"
  },
  "homepage": "https://github.com/GusCaplan/retrocord#readme",
  "nexe": {
    "input": "./src/index.js",
    "output": "./build/retrocord^$",
    "temp": "tmp",
    "browserify": {
      "requires": [],
      "excludes": [],
      "paths": []
    },
    "runtime": {
      "framework": "node",
      "version": "latest",
      "js-flags": "--use_strict",
      "ignoreFlags": true
    }
  }
}
